# Copy to .env and edit if needed
MGC_DB_PATH=./data/db.sqlite
MGC_DATA_DIR=./data
MGC_LOG_DIR=./logs

# Optional (later)
# STRIPE_SECRET_KEY=
# X_API_KEY=

# Which music generator provider to use: stub | riffusion
MGC_PROVIDER=stub

# Riffusion server endpoint (if using riffusion)
RIFFUSION_URL=http://127.0.0.1:3013/run_inference

# Riffusion generation params (lower steps = faster)
RIFFUSION_STEPS=25
RIFFUSION_GUIDANCE=7.0
RIFFUSION_DENOISE=0.75
